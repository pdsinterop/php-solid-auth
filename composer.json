    "files": [
      "src/Exceptions.inc.php"
    ],
    "psr-4": {
      "Pdsinterop\\Solid\\Auth\\": "src/"
    }
  },
  "config": {
    "bin-dir": "./bin",
    "platform": {
      "php": "7.2",
      "ext-json": "1",
      "ext-mbstring": "1",
      "ext-openssl": "1"
    },
    "sort-packages": true
