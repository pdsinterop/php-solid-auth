    "php": ">=7.2",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "league/oauth2-server": "^8.0"
  },
  "require-dev": {
    "laminas/laminas-diactoros": "^2.3",
    "lcobucci/jwt": "^3.3"
  },
  "type": "library"
